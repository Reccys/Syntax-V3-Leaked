--noclip
local Collider
function Noclip()
    player.Humanoid.Died:Connect(function() Collider:Disconnect() return end)
    for i, v in pairs(player:GetDescendants()) do
        if v:IsA("BasePart") then
            v.CanCollide = false
        end
    end
end
Collider = game:GetService("RunService").Stepped:Connect(Noclip)

--deleting Attachments in Torso
--R6 Attachments
if game.Players.LocalPlayer.Character.Humanoid.RigType == Enum.HumanoidRigType.R6 then
player["Torso"]["Right Hip"]:Destroy()
player["Torso"]["Left Shoulder"]:Destroy()
player["Torso"]["Left Hip"]:Destroy()
player["Torso"]["Right Shoulder"]:Destroy()
elseif game.Players.LocalPlayer.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
--R15 Attachments
player.RightUpperArm["RightShoulder"]:Destroy()
player.RightLowerArm["RightElbow"]:Destroy()
player.RightHand["RightWrist"]:Destroy()
player.LeftUpperArm["LeftShoulder"]:Destroy()
player.LeftLowerArm["LeftElbow"]:Destroy()
player.LeftHand["LeftWrist"]:Destroy()
player.LeftUpperLeg["LeftHip"]:Destroy()
player.LeftLowerLeg["LeftKnee"]:Destroy()
player.LeftFoot["LeftAnkle"]:Destroy()
player.RightUpperLeg["RightHip"]:Destroy()
player.RightLowerLeg["RightKnee"]:Destroy()
player.RightFoot["RightAnkle"]:Destroy()
player.UpperTorso["Waist"]:Destroy()
player.LowerTorso["Root"]:Destroy()
end


