flingtab.Name = "fling-tab"
flingtab.Parent = tabs
flingtab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
flingtab.BackgroundTransparency = 1.000
flingtab.BorderSizePixel = 0
flingtab.Selectable = false
flingtab.Size = UDim2.new(1, 0, 0.6, 0)
flingtab.Visible = false
flingtab.ZIndex = 3
flingtab.ScrollBarThickness = 4
flingtab.ScrollBarImageColor3 = Color3.fromRGB(0,0,0)

local flings = {}

function flings:addScript(name,description,callback)
	local script_3 = Instance.new("Frame")
	local scriptback_3 = Instance.new("Frame")
	local UICorner_4 = Instance.new("UICorner")
	local scriptname_3 = Instance.new("TextLabel")
	local scriptdescription_3 = Instance.new("TextLabel")
	local ImageButton_3 = Instance.new("ImageButton")

	callback = typeof(callback) == "function" and callback or function() end

	script_3.Name = "script"
	script_3.Parent = flingtab
	script_3.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	script_3.BackgroundTransparency = 1.000
	script_3.BorderSizePixel = 0
	script_3.Size = UDim2.new(0, 378, 0, 80)

	scriptback_3.Name = "script-back"
	scriptback_3.Parent = script_3
	scriptback_3.AnchorPoint = Vector2.new(0.5, 0.5)
	scriptback_3.BackgroundColor3 = shared.colors["Scripts"] or Color3.fromRGB(30,30,30)
	scriptback_3.BorderSizePixel = 0
	scriptback_3.Position = UDim2.new(0.5, 0, 0.5, 0)
	scriptback_3.Size = UDim2.new(0, 366, 0, 70)
	scriptback_3.BackgroundTransparency = shared.transparency["Scripts"] or 0

	UICorner_4.CornerRadius = UDim.new(0, 4)
	UICorner_4.Parent = scriptback_3

	scriptname_3.Name = "script-name"
	scriptname_3.Parent = scriptback_3
	scriptname_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	scriptname_3.BackgroundTransparency = 1.000
	scriptname_3.BorderSizePixel = 0
	scriptname_3.Position = UDim2.new(0.0245901644, 0, 0, 0)
	scriptname_3.Size = UDim2.new(0, 201, 0, 22)
	scriptname_3.Font = Enum.Font.GothamSemibold
	scriptname_3.Text = name
	scriptname_3.TextColor3 = shared.colors["Text"] or Color3.fromRGB(255,255,255)
	scriptname_3.TextSize = 14.000
	scriptname_3.TextXAlignment = Enum.TextXAlignment.Left

	scriptdescription_3.Name = "script-description"
	scriptdescription_3.Parent = scriptback_3
	scriptdescription_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	scriptdescription_3.BackgroundTransparency = 1.000
	scriptdescription_3.BorderSizePixel = 0
	scriptdescription_3.Position = UDim2.new(0.0245901644, 0, 0.314285725, 0)
	scriptdescription_3.Size = UDim2.new(0, 241, 0, 39)
	scriptdescription_3.Font = Enum.Font.GothamSemibold
	scriptdescription_3.Text = description
	scriptdescription_3.TextColor3 = shared.colors["Description"] or Color3.fromRGB(125,125,125)
	scriptdescription_3.TextSize = 12.000
	scriptdescription_3.TextXAlignment = Enum.TextXAlignment.Left
	scriptdescription_3.TextYAlignment = Enum.TextYAlignment.Top

	ImageButton_3.Parent = scriptback_3
	ImageButton_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	ImageButton_3.BackgroundTransparency = 1.000
	ImageButton_3.Position = UDim2.new(0.90437156, 0, 0.357142866, 0)
	ImageButton_3.Size = UDim2.new(0, 20, 0, 20)
	ImageButton_3.Image = "rbxassetid://7072720722"
	ImageButton_3.ImageColor3 = shared.colors["Text"] or Color3.fromRGB(255,255,255)
	ImageButton_3.MouseButton1Click:connect(function()
		syntax:playSound("rbxassetid://4499400560",0.5,false,false,game.Workspace)
		pcall(callback)
	end)
	spawn(function()
		while wait(0.1) do
			scriptback_3.BackgroundColor3 = shared.colors["Scripts"] or Color3.fromRGB(30,30,30)
			scriptback_3.BackgroundTransparency = shared.transparency["Scripts"] or 0
			ImageButton_3.ImageColor3 = shared.colors["Text"] or Color3.fromRGB(255,255,255)
			ImageButton_3.ImageTransparency = shared.transparency["Text"] or 0
			scriptname_3.TextColor3 = shared.colors["Text"] or Color3.fromRGB(255,255,255)
			scriptdescription_3.TextColor3 = shared.colors["Description"] or Color3.fromRGB(125,125,125)
			scriptname_3.TextTransparency = shared.transparency["Text"] or 0
			scriptdescription_3.TextTransparency = shared.transparency["Description"] or 0
		end
	end)
end

