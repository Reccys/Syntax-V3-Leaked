flings:addScript("Battle Rifle","Controls\n- Click to shoot, X to stab",function()
local a=game.Players.LocalPlayer.Character;player=game.Players.LocalPlayer;player.Character.Humanoid.WalkSpeed=0;for b=0,20 do wait()a.HumanoidRootPart.RotVelocity=Vector3.new(0,40,0)a.HumanoidRootPart.Velocity=Vector3.new(0,30,0)end;wait(0.5)player.Character.Humanoid.WalkSpeed=16;power=500;game:GetService("RunService").Stepped:connect(function()game.Players.LocalPlayer.Character.Head.CanCollide=false;game.Players.LocalPlayer.Character["Left Leg"].CanCollide=false;game.Players.LocalPlayer.Character["Right Leg"].CanCollide=false end)wait(.1)local c=Instance.new("BodyThrust")c.Parent=game.Players.LocalPlayer.Character.Torso;c.Force=Vector3.new(power,0,power)c.Location=game.Players.LocalPlayer.Character.HumanoidRootPart.Position;game.Players.LocalPlayer.Character["SeeMonkey"].Handle.Mesh:Destroy()local d=game.Players.LocalPlayer.Character.Animate;HumanDied=false;local e=1;function Align(f,g,h,i)local j=Instance.new('AlignPosition',g)j.Name="AliP_"..e;j.ApplyAtCenterOfMass=true;j.MaxForce=5772000;j.MaxVelocity=math.huge/9e110;j.ReactionForceEnabled=false;j.Responsiveness=200;j.RigidityEnabled=true;local k=Instance.new('AlignOrientation',g)k.Name="AliO_"..e;k.MaxAngularVelocity=math.huge/9e110;k.MaxTorque=5772000;k.PrimaryAxisOnly=false;k.ReactionTorqueEnabled=false;k.Responsiveness=200;k.RigidityEnabled=true;local l=Instance.new('Attachment',g)l.Name="Ath_"..e;local m=Instance.new('Attachment',f)m.Name="Ath_"..e;l.Orientation=i or Vector3.new(0,0,0)l.Position=h or Vector3.new(0,0,0)j.Attachment1=l;j.Attachment0=m;k.Attachment1=l;k.Attachment0=m;e=e+1;spawn(function()while wait()do f.RotVelocity=Vector3.new(0,0,0)f.Velocity=Vector3.new(30,0,0)f.AssemblyLinearVelocity=-f.Velocity end end)game:GetService("RunService").Stepped:Connect(function()for n,o in pairs(game:GetService("Players").LocalPlayer.Character:GetChildren())do if o:IsA("Part")then o.CanCollide=false end;if o:IsA("Accessory")then o.Handle.CanCollide=false end end end)return{j,k,l,m}end;if _G.netted~=true then