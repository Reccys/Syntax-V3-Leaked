--Checking
if shared.syntax_v3_loaded == true then
	syntax:warn("Syntax already running.")
	return
end

pcall(function() 
	shared.syntax_v3_loaded = true 
end)

lplayer = game:GetService("Players").LocalPlayer

local Mouse = lplayer:GetMouse()

