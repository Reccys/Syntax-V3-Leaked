		end)
	olds:addScript("Venom UPDATED","Controls\n-Click to attack, Q to run",function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/memeboiyoot/Syntax-v3-free/main/Venom", true))() 

	end)
	olds:addScript("Sex Doll","Controls\n-F to sex with the doll / E for the doll suck your dick",function()
		print("trolled")
	end)
		olds:addScript("FLOPPA","Controls\n-None",function()
		loadstring(game:GetObjects("rbxassetid://7837583923")[1].Source)()
		end)
	olds:addScript("Jason Voorhees","Controls\n-Click to attack",function()
		loadstring(game:GetObjects("rbxassetid://7838004475")[1].Source)()
	end)
		olds:addScript("Plane","Controls\n-None",function()
		loadstring(game:GetObjects("rbxassetid://7443402710")[1].Source)()
		end)
	olds:addScript("Knife","Controls\n-Click to slash",function()
		loadstring(game:GetObjects("rbxassetid://7572066757")[1].Source)()
	end)
end

spawn(function()
	while wait(0.1) do
		version.TextColor3 = shared.colors["Version"] or Color3.fromRGB(0,255,149)
		icon.ImageColor3 = shared.colors["Icons"] or Color3.fromRGB(0,255,149)
		icon_2.ImageColor3 = shared.colors["Icons"] or Color3.fromRGB(0,255,149)
		icon_3.ImageColor3 = shared.colors["Icons"] or Color3.fromRGB(0,255,149)
		icon_4.ImageColor3 = shared.colors["Icons"] or Color3.fromRGB(0,255,149)
		icon_5.ImageColor3 = shared.colors["Icons"] or Color3.fromRGB(0,255,149)

		main.BackgroundColor3 = shared.colors["Back"] or Color3.fromRGB(25,25,25)

		drop.ImageColor3 = shared.colors["Glow"] or Color3.fromRGB(0,0,0)

		tabs_sel.BackgroundColor3 = shared.colors["TabList"] or Color3.fromRGB(30,30,30)

		name.TextColor3 = shared.colors["Text"] or Color3.fromRGB(255,255,255)
		TextLabel.TextColor3 = shared.colors["Text"] or Color3.fromRGB(255,255,255)
		TextLabel_2.TextColor3 = shared.colors["Text"] or Color3.fromRGB(255,255,255)
		TextLabel_3.TextColor3 = shared.colors["Text"] or Color3.fromRGB(255,255,255)
		TextLabel_4.TextColor3 = shared.colors["Text"] or Color3.fromRGB(255,255,255)
		TextLabel_5.TextColor3 = shared.colors["Text"] or Color3.fromRGB(255,255,255)
		TextLabel_7.TextColor3 = shared.colors["Text"] or Color3.fromRGB(255,255,255)

		TextLabel_8.TextColor3 = shared.colors["Description"] or Color3.fromRGB(125,125,125)
	end
end)
spawn(function()
	while wait(0.1) do
		tabs_sel.BackgroundTransparency = shared.transparency["TabList"] or 0

		main.BackgroundTransparency = shared.transparency["Back"] or 0

		drop.ImageTransparency = shared.transparency["Glow"] or 0.5

		name.TextTransparency = shared.transparency["Text"] or 0
		TextLabel.TextTransparency = shared.transparency["Text"] or 0
		TextLabel_2.TextTransparency = shared.transparency["Text"] or 0
		TextLabel_3.TextTransparency = shared.transparency["Text"] or 0
		TextLabel_4.TextTransparency = shared.transparency["Text"] or 0
		TextLabel_5.TextTransparency = shared.transparency["Text"] or 0
		TextLabel_7.TextTransparency = shared.transparency["Text"] or 0

		TextLabel_8.TextTransparency = shared.transparency["Description"] or 0

		version.TextTransparency = shared.transparency["Version"] or 0

		icon.ImageTransparency = shared.transparency["Icons"] or 0
		icon_2.ImageTransparency = shared.transparency["Icons"] or 0
		icon_3.ImageTransparency = shared.transparency["Icons"] or 0
		icon_4.ImageTransparency = shared.transparency["Icons"] or 0
		icon_5.ImageTransparency = shared.transparency["Icons"] or 0
	end
end)


