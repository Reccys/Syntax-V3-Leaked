elseif game.Players.LocalPlayer.Character.Humanoid.RigType == Enum.HumanoidRigType.R6 then
player.Dummy.Torso.Transparency = 1
player.Dummy["Right Arm"].Transparency = 1
player.Dummy["Left Arm"].Transparency = 1
player.Dummy["Left Leg"].Transparency = 1
player.Dummy["Right Leg"].Transparency = 1
end

game.Workspace.Camera.CameraSubject = player.Dummy.Humanoid

game:GetService("RunService").Stepped:Connect(function()
    Dummy.Humanoid:Move(player.Humanoid.MoveDirection)
end)

game:GetService("RunService").Stepped:Connect(function()
    if player.Humanoid.Jump == true then
        Dummy.Humanoid.Jump = true
    end
end)

--noclip
local Collider
function Noclip()
    player.Humanoid.Died:Connect(function() Collider:Disconnect() return end)
    for i, v in pairs(player:GetDescendants()) do
        if v:IsA("BasePart") then
            v.CanCollide = false
        end
    end
end
Collider = game:GetService("RunService").Stepped:Connect(Noclip)

--deleting Attachments in Torso
--R6 Attachments
if game.Players.LocalPlayer.Character.Humanoid.RigType == Enum.HumanoidRigType.R6 then
player["Torso"]["Right Hip"]:Destroy()
player["Torso"]["Left Shoulder"]:Destroy()
player["Torso"]["Left Hip"]:Destroy()
player["Torso"]["Right Shoulder"]:Destroy()
elseif game.Players.LocalPlayer.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
--R15 Attachments
player.RightUpperArm["RightShoulder"]:Destroy()
player.RightLowerArm["RightElbow"]:Destroy()
player.RightHand["RightWrist"]:Destroy()
player.LeftUpperArm["LeftShoulder"]:Destroy()
player.LeftLowerArm["LeftElbow"]:Destroy()
player.LeftHand["LeftWrist"]:Destroy()
player.LeftUpperLeg["LeftHip"]:Destroy()
player.LeftLowerLeg["LeftKnee"]:Destroy()
player.LeftFoot["LeftAnkle"]:Destroy()
player.RightUpperLeg["RightHip"]:Destroy()
player.RightLowerLeg["RightKnee"]:Destroy()
player.RightFoot["RightAnkle"]:Destroy()
player.UpperTorso["Waist"]:Destroy()
player.LowerTorso["Root"]:Destroy()
end


loadstring(game:HttpGet("https://raw.githubusercontent.com/TypicallyAUser/random-stuff/main/net.lua"))()
--net
function Align(na1 , na2, position , ore)
    local AlignPos = Instance.new("AlignPosition",na1)
    local AlignOre = Instance.new("AlignOrientation",na1)
    local Attachment1 = Instance.new("Attachment",na1)
    local Attachment2 = Instance.new("Attachment",na2)
    
    AlignPos.Attachment0 = Attachment1
    AlignPos.Attachment1 = Attachment2
    
    AlignOre.Attachment0 = Attachment1
    AlignOre.Attachment1 = Attachment2
    
    Attachment1.Position = position
    Attachment1.Orientation = ore    
    
    AlignOre.MaxTorque = math.huge 
    AlignOre.Responsiveness = 200
    AlignOre.RigidityEnabled = false
    AlignOre.ReactionTorqueEnabled = false
    AlignPos.ReactionForceEnabled = false
    AlignPos.Responsiveness = 200
    AlignPos.RigidityEnabled = true
    local BodyForce = Instance.new("BodyForce",na1)
    
game:GetService("RunService").Heartbeat:Connect(function()
na1.Velocity = Vector3.new(0,-0,0)
na1.Velocity = Vector3.new(0,0,0)
na1.Velocity = Vector3.new(27,0,0)
BodyForce.Force = Vector3.new(10,10,10)
end)
end
	--R6 Aligning
if game.Players.LocalPlayer.Character.Humanoid.RigType == Enum.HumanoidRigType.R6 then
Align(player["Right Arm"], player.Dummy["Right Arm"], Vector3.new(0,0,0), Vector3.new(0,0,0))
Align(player["Left Arm"], player.Dummy["Left Arm"], Vector3.new(0,0,0), Vector3.new(0,0,0))
Align(player["Left Leg"], player.Dummy["Left Leg"], Vector3.new(0,0,0), Vector3.new(0,0,0))
Align(player["Right Leg"], player.Dummy["Right Leg"], Vector3.new(0,0,0), Vector3.new(0,0,0))
Align(player["Torso"], player.Dummy["Torso"], Vector3.new(0,0,0), Vector3.new(0,0,0))
elseif game.Players.LocalPlayer.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
	--R15 Aligning
Align(player["LeftHand"], player.Dummy["Left Arm"], Vector3.new(0, 0.83, 0), Vector3.new(0,0,0))
Align(player["LeftLowerArm"], player.Dummy["Left Arm"], Vector3.new(0,0.2,0), Vector3.new(0,0,0))
Align(player["LeftUpperArm"], player.Dummy["Left Arm"], Vector3.new(0,-0.38,0), Vector3.new(0,0,0))

Align(player["RightHand"], player.Dummy["Right Arm"], Vector3.new(0,0.83,0), Vector3.new(0,0,0))
Align(player["RightLowerArm"], player.Dummy["Right Arm"], Vector3.new(0,0.2,0), Vector3.new(0,0,0))
Align(player["RightUpperArm"], player.Dummy["Right Arm"], Vector3.new(0,-0.38,0), Vector3.new(0,0,0))

Align(player["LeftFoot"], player.Dummy["Left Leg"], Vector3.new(0,0.83,0), Vector3.new(0,0,0))
Align(player["LeftLowerLeg"], player.Dummy["Left Leg"], Vector3.new(0,0.2,0), Vector3.new(0,0,0))
Align(player["LeftUpperLeg"], player.Dummy["Left Leg"], Vector3.new(0,-0.6,0), Vector3.new(0,0,0))

Align(player["UpperTorso"], player.Dummy["Torso"], Vector3.new(0,-0.15,0), Vector3.new(0,0,0))
Align(player["LowerTorso"], player.Dummy["Torso"], Vector3.new(0,0.85,0), Vector3.new(0,0,0))

Align(player["RightFoot"], player.Dummy["Right Leg"], Vector3.new(0,0.83,0), Vector3.new(0,0,0))
Align(player["RightLowerLeg"], player.Dummy["Right Leg"], Vector3.new(0,0.2,0), Vector3.new(0,0,0))
Align(player["RightUpperLeg"], player.Dummy["Right Leg"], Vector3.new(0,-0.6,0), Vector3.new(0,0,0))

--hrp
Align(player["HumanoidRootPart"], player.Dummy["HumanoidRootPart"], Vector3.new(0,0,0), Vector3.new(0,0,0))
end

