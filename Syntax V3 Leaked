local anims = {}

function anims:addScript(name,description,callback)
	local script = Instance.new("Frame")
	local scriptback = Instance.new("Frame")
	local UICorner_2 = Instance.new("UICorner")
	local scriptname = Instance.new("TextLabel")
	local scriptdescription = Instance.new("TextLabel")
	local ImageButton = Instance.new("ImageButton")

	callback = typeof(callback) == "function" and callback or function() end

	script.Name = "script"
	script.Parent = animstab
	script.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	script.BackgroundTransparency = 1.000
	script.BorderSizePixel = 0
	script.Size = UDim2.new(0, 378, 0, 80)

	scriptback.Name = "script-back"
	scriptback.Parent = script
	scriptback.AnchorPoint = Vector2.new(0.5, 0.5)
	scriptback.BackgroundColor3 = shared.colors["Scripts"] or Color3.fromRGB(30,30,30)
	scriptback.BorderSizePixel = 0
	scriptback.Position = UDim2.new(0.5, 0, 0.5, 0)
	scriptback.Size = UDim2.new(0, 366, 0, 70)
	scriptback.BackgroundTransparency = shared.transparency["Scripts"] or 0

	UICorner_2.CornerRadius = UDim.new(0, 4)
	UICorner_2.Parent = scriptback

	scriptname.Name = "script-name"
	scriptname.Parent = scriptback
	scriptname.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	scriptname.BackgroundTransparency = 1.000
	scriptname.BorderSizePixel = 0
	scriptname.Position = UDim2.new(0.0245901644, 0, 0, 0)
	scriptname.Size = UDim2.new(0, 201, 0, 22)
	scriptname.Font = Enum.Font.GothamSemibold
	scriptname.Text = name
	scriptname.TextColor3 = shared.colors["Text"] or Color3.fromRGB(255,255,255)
	scriptname.TextSize = 14.000
	scriptname.TextXAlignment = Enum.TextXAlignment.Left

	scriptdescription.Name = "script-description"
	scriptdescription.Parent = scriptback
	scriptdescription.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	scriptdescription.BackgroundTransparency = 1.000
	scriptdescription.BorderSizePixel = 0
	scriptdescription.Position = UDim2.new(0.0245901644, 0, 0.314285725, 0)
	scriptdescription.Size = UDim2.new(0, 241, 0, 39)
	scriptdescription.Font = Enum.Font.GothamSemibold
	scriptdescription.Text = description
	scriptdescription.TextColor3 = shared.colors["Description"] or Color3.fromRGB(125,125,125)
	scriptdescription.TextSize = 12.000
	scriptdescription.TextXAlignment = Enum.TextXAlignment.Left
	scriptdescription.TextYAlignment = Enum.TextYAlignment.Top

	ImageButton.Parent = scriptback
	ImageButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	ImageButton.BackgroundTransparency = 1.000
	ImageButton.Position = UDim2.new(0.90437156, 0, 0.357142866, 0)
	ImageButton.Size = UDim2.new(0, 20, 0, 20)
	ImageButton.Image = "rbxassetid://7072720722"
	ImageButton.ImageColor3 = shared.colors["Text"] or Color3.fromRGB(255,255,255)
	ImageButton.MouseButton1Click:Connect(function()
		syntax:playSound("rbxassetid://4499400560",0.5,false,false,game.Workspace)
		pcall(callback)
	end)
	spawn(function()
		while wait(0.1) do
			scriptback.BackgroundColor3 = shared.colors["Scripts"] or Color3.fromRGB(30,30,30)
			scriptback.BackgroundTransparency = shared.transparency["Scripts"] or 0
			ImageButton.ImageColor3 = shared.colors["Text"] or Color3.fromRGB(255,255,255)
			ImageButton.ImageTransparency = shared.transparency["Text"] or 0
			scriptname.TextColor3 = shared.colors["Text"] or Color3.fromRGB(255,255,255)
			scriptdescription.TextColor3 = shared.colors["Description"] or Color3.fromRGB(125,125,125)
			scriptname.TextTransparency = shared.transparency["Text"] or 0
			scriptdescription.TextTransparency = shared.transparency["Description"] or 0
		end
	end)
end

